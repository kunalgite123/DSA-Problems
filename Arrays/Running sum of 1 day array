//time complexity = O(n)
//space complexity =O(1)


#include <iostream>
using namespace std;

int returnsum(int arr[],int n){
   for(int i = 1; i<n; i++){
      arr[i]=arr[i-1] +arr[i];
}
for(int i =0; i<n; i++){
   cout<<arr[i]<<",";
}
}
int main(){
   int arr[]={1,1,1,1,1,1};
   int n = sizeof(arr)/sizeof(int);
   returnsum(arr,n);
   return 0;
}
